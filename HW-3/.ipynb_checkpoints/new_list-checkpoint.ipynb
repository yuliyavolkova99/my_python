   "source": [
    "from math import sqrt \n",
    "a = [2, 4, 9, 16, 25]\n",
    "\n",
    "# на основе цикла for\n",
    "for i in a:\n",
    "    print(sqrt(i), end = ' ')\n",
    "\n",
    "# на основе функции map\n",
    "print('\\n',list(map((lambda i: sqrt(i)),a)))\n",
    "\n",
    "# в виде генератора списка\n",
    "print([sqrt(i) for i in a])"
   ]
  }
